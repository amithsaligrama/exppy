base_x = raw_input("\nWhat integer base do you want to have your start number be before I turn it into base 10? ")
base_x_num = raw_input("\nWhat number in base " + str(base_x) + " do you want to convert to base 10? ")
def convert_from_base(num,base):
  return int(num, base)
print "\nThis is your number in base 10: %r\n" % (convert_from_base(base_x_num, int(base_x)))
